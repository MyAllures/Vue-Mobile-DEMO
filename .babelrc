{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1% in CN", "not ie <= 8", "safari >= 7"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime", {
    name: 'vux-ui'
  }],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["transform-vue-jsx", "istanbul"]
    }
  }
}
